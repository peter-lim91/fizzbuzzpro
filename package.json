{
  "name": "fizzbuzzpro",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start-next": "next start",
    "start-e": "node server",
    "start": "set NODE_ENV=production && node server",
    "knex": "knex --knexfile server/db/knexfile.js",
    "db:delete": "rm server/db/dev.sqlite3",
    "db:migrate": "npm run knex migrate:latest",
    "db:seed": "npm run knex seed:run",
    "db:reset": "npm run knex migrate:rollback && npm run db:migrate && npm run db:seed"
  },
  "dependencies": {
    "@google-cloud/vision": "^2.3.1",
    "@sendgrid/mail": "^7.4.2",
    "axios": "^0.21.1",
    "connect-session-knex": "^2.0.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "gm": "^1.23.1",
    "knex": "^0.95.2",
    "multer": "^1.4.2",
    "next": "10.0.8",
    "pg": "^8.5.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "sqlite3": "^5.0.2"
  },
  "devDependencies": {
    "eslint": "^7.22.0",
    "prettier": "^2.2.1"
  }
}
